{
  "name": "@happysanta/vk-app-ui",
  "version": "1.0.7",
  "main": "dist/vkappui.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack -p",
    "build:guide": "npx styleguidist build",
    "watch:guide": "npx styleguidist server",
    "prepublish": "npm run build && npm run build:guide",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HappySanta/vk-app-ui.git"
  },
  "author": "happysanta",
  "jsnext:main": "src/index.js",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx",
      "transform-class-properties"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HappySanta/vk-app-ui/issues"
  },
  "homepage": "https://github.com/HappySanta/vk-app-ui#readme",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "moment": "^2.22.2",
    "@happysanta/vk-apps-sdk": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.27.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react-styleguidist": "^7.1.0",
    "sass-loader": "^7.0.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "description": "Элементы интерфейса в стиле ВК",
  "dependencies": {
    "@happysanta/vk-apps-sdk": "^3.0.0",
    "@vkontakte/vk-bridge": "^2.0.8",
    "moment": "^2.24.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
